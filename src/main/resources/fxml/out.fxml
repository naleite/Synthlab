<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<fx:root prefHeight="250.0" prefWidth="150.0" styleClass="module" type="javafx.scene.layout.BorderPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
    <center>
        <VBox alignment="CENTER" BorderPane.alignment="CENTER" spacing="20">
            <Button id="muteBtn" fx:id="muteBtn" mnemonicParsing="false" onAction="#onMuteBtnClick" styleClass="mute-on" BorderPane.alignment="CENTER"/>
            <VBox fx:id="knobs" alignment="CENTER"/>
            <Label fx:id="attenuation" text="0">
                <font>
                    <Font size="20.0"/>
                </font>
            </Label>
        </VBox>
   </center>
</fx:root>
